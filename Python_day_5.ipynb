    "def log_decorator(func):\n",
    "    def wrapper():\n",
    "        print(\"Function is about to run...\")\n",
    "        func()\n",
    "        print(\"Function has finished.\")\n",
    "    return wrapper\n",
    "\n",
    "@log_decorator\n",
    "def scan():\n",
    "    print(\"Scanning target...\")\n",
    "\n",
    "scan()"
